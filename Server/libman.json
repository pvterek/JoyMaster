{
  "version": "1.0",
  "defaultProvider": "unpkg",
  "libraries": [
    {
      "library": "@microsoft/signalr@latest",
      "destination": "wwwroot/lib/signalr",
      "files": [
        "dist/browser/signalr.js",
        "dist/browser/signalr.min.js"
      ]
    },
    {
      "library": "jquery@latest",
      "destination": "wwwroot/lib/jquery",
      "files": [
        "dist/jquery.js",
        "dist/jquery.min.js"
      ]
    }
  ]
}